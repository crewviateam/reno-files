{
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/public/$1"
        }
    ],
    "headers": [
        {
            "source": "/:path*.js",
            "headers": [
                {
                    "key": "Content-Type",
                    "value": "text/plain"
                }
            ]
        },
        {
            "source": "/:path*.css",
            "headers": [
                {
                    "key": "Content-Type",
                    "value": "text/plain"
                }
            ]
        }
    ]
}